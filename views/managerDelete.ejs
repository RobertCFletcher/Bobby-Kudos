<!doctype html>
<html lang="en">

<head>
  <%- include('partials/head') %>
  <link href="https://fonts.googleapis.com/css?family=Spartan:400,700&display=swap" rel="stylesheet"> 
  <%- include('partials/head') %>
  <link rel="stylesheet" type="text/css" href="../../assets/managerModify.css">
  <%- include('partials/navbar') %>
</head>


<body>

    <div id='titleSpace'></div>

     <div id="tables">
     <table class="table table-striped table-dark">
      <thead>
        <tr>    
          <th> <h3>Modify The Following Manager </h3> </th>
        </tr>
        <tr>
          <th>Manager ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Creation Date</th>
          <th>Created By</th>
        </tr>
      </thead>
        <tbody>
          <tr>
            <td> <%= managerData.userid %> </td> 
            <td> <%= managerData.firstname %> </td>
            <td> <%= managerData.lastname %> </td>
            <td> <%= managerData.email %> </td>
            <td> <%= managerData.createdon.Time %> </td>
            <td> <%= managerData.createdby %> </td>
          </tr>  
        </tbody>
      </table>
    </div>
	
      <div id='titleSpace'></div>

      <li>
        <h4>Are you sure you want to delete this user?</h4>
      </li>  
      <a href="/admin/deletemanager/bye/<%= managerData.userid %>"><button id="submitButton" type="button">Delete Account</button></a>
      <a href="/admin"><button id="cancelButton" type="button">Cancel</button></a>

     
<hr>

  <%- include('partials/footer') %>
  <%- include('partials/footscripts') %>
</body>
</html>
